

default: 
	(cd fe;        $(MAKE) $@)
	(cd mfc_io;    $(MAKE) $@)

sphere_lib:
	(cd sphere; sh src/scripts/install.sh)

install:
	(cd fe;        $(MAKE) $@)
	(cd mfc_io;    $(MAKE) $@)

clean:
	(cd fe;        $(MAKE) $@)
	(cd mfc_io;    $(MAKE) $@)

bare:
	(cd fe;        $(MAKE) $@)
	(cd mfc_io;    $(MAKE) $@)

